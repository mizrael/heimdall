{
  "compilerOptions": {
    "skipLibCheck": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "outDir": "./app/bin/",
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
